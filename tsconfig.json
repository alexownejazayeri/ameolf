{
  "compilerOptions": {
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["es2023", "dom"],
    "target": "es5",
    "module": "CommonJS",
    "moduleResolution": "NodeNext",
    "noImplicitAny": false,
    "noEmitOnError": false,
    "removeComments": true,
    "skipLibCheck": true,
    "strict": true,
    "strictPropertyInitialization": false,
  },
  "include": ["app.ts"],
  "exclude": ["node_modules"]
}
